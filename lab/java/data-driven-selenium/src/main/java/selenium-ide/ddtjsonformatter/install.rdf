<?xml version="1.0" encoding="UTF-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
		<em:id>ddtjsonformatter@zuora.com</em:id>
		<em:name>JSON Formatter for Zuora Selenium DDT</em:name>
		<em:version>1.0</em:version>
		<em:type>2</em:type>
		<em:creator>Leo Liu</em:creator>
		<em:description>Export selenium ide commands to Zuora Selenium DDT test case</em:description>
		<em:homepageURL>http://www.zuora.com/</em:homepageURL>
		<em:optionsURL>chrome://ddtjsonformatter/content/options.xul</em:optionsURL>

		<!-- Its a firefox plugin -->
		<em:targetApplication>

			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>3.6</em:minVersion>
				<em:maxVersion>14.*</em:maxVersion>
			</Description>
		</em:targetApplication>
		
		<!-- This is an Se-IDE plugin, so we need to specify it as a requirement -->
		<em:requires>   
			<Description>
				<em:id>{a6fd85ed-e919-4a43-a5af-8da18bda539f}</em:id>
				<em:minVersion>1.0.4-SNAPSHOT</em:minVersion>
				<em:maxVersion>1.*</em:maxVersion>
			</Description>  
		</em:requires>		
	</Description>
</RDF>